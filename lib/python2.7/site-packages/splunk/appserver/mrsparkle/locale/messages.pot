"_query=( ( \"host::mylaptop\" AND \"source::syslog.log\" AND "
"\"sourcetype::syslog\" ) OR ( \"host::bobslaptop\" AND \"source::bob-"
"syslog.log\" AND \"sourcetype::syslog\" ) )"
msgstr ""

