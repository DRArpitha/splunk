msgstr "Imposta il formato orario per i termini starttime e endtime."

#: cfg/bundles/default/searchbnf.conf.in:2754
msgid "Search for events from the specified sourcetype"
msgstr "Ricerca eventi dallo specificato sourcetype"

#: cfg/bundles/default/searchbnf.conf.in:2758
msgid "Search for events from the specified host"
msgstr "Ricerca eventi dallo specificato host"

#: cfg/bundles/default/searchbnf.conf.in:2762
