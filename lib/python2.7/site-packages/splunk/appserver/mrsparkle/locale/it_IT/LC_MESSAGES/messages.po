#: cfg/bundles/default/searchbnf.conf.in:2709
msgid "Keep only search results that have the specified \"src\" or \"dst\" values."
msgstr "Tiene solo i risultati della search che hanno gli specificati valori di \"src\" o \"dst\"."

#: cfg/bundles/default/searchbnf.conf.in:2735
msgid "Search the specified index instead of the default index"
msgstr "Ricerca nell'indice specificato piuttosto che nell'indice di default"

#: cfg/bundles/default/searchbnf.conf.in:2748
