msgstr "最近 15 分钟"

#: cfg/bundles/default/times.conf.in:16
msgid "Last 60 minutes"
msgstr "最近 60 分钟"

#: cfg/bundles/default/times.conf.in:17
msgid "in the last 60 minutes"
msgstr "最近 60 分钟"

#: cfg/bundles/default/times.conf.in:23
msgid "Last 4 hours"
msgstr "最近 4 小时"

#: cfg/bundles/default/times.conf.in:24
msgid "in the last 4 hours"
msgstr "最近 4 小时"

